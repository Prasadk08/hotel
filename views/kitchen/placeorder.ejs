<% layout('/layout/boilerplate2') -%>
<div class="row">
    <div class="col-8 offset-2">
      <h1>Adding New Waiter</h1>
      <form method="POST" action="/listing/new" novalidate class="needs-validation" enctype="multipart/form-data">
        <div class="mb-2">
          <label for="title" class="form-label">Enter Table No.</label>
          <input type="text" name="listing[title]" class="form-control" placeholder="Add your Title" required>
          <div class="invalid-feedback">Please enter a title</div>
        </div>
        <br>
        <button class="btn btn-primary btn-decor">Submit</button>
      </form>
    </div>
  </div>

